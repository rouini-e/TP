pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
    
        stage('Build') {
            steps {
                echo 'dev'
            }
        }
stage('Deploy') {
            steps {
                echo 'mise en production'
            }
        }
    
    
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
stage('Release') {
            steps {
                echo 'Lancement'
            }
        }
    
    }
}
